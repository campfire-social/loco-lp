        {% comment %}
        {% elsif forloop.index == 2 %}
        {% assign gridClass = "lg:row-span-1 lg:col-span-1" %}
        {% elsif forloop.index == 3 %}
        {% assign gridClass = "lg:row-span-1 lg:col-span-1" %}
        {% endcomment %}
        <li class="{{gridClass}} h-full">
          <article class="relative flex flex-col bg-black h-full">
            <div class="aspect-w-3 aspect-h-2 lg:h-full top-0 left-0 w-full">
              <div class="bg-gradient-to-t opacity-80 from-black z-20 w-full h-full"></div>
              {% assign _recipeAlt = "photo of" | append: recipe.name %}
              {% image recipe.image, _recipeAlt, "recipe-image"  %}
            </div>
            <div class="absolute z-30 bottom-0 left-0 w-full">
              <div class="flex items-center">
               <div class="p-6 text-white">
                <h3 class="font-serif font-bold text-2xl">{{recipe.name}}</h3>
                <div class="inline-flex">
                  <p>
                    <span class="sr-only">Difficulty: </span>
                    {{recipe.difficulty}}
                  </p>
                  <span class="mx-3">|</span>
                  <p>
                    <span class="sr-only">Time: </span>
                    {{recipe.time}}
                  </p>
                </div>
              </div>
                <a class="inline-block ml-auto text-white p-6 font-bold uppercase tracking-wider" href="{{recipe.url}}" target="_blank">
                  <span>Watch Video</span>
                </a>
              </div>
             
            </div>
          </article>
        </li>
        {% endfor %}
      </ul>
      {% endif %}
    </div>
  </div>
</section>


